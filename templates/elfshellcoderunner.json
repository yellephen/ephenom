{
    "categories": [
        "elf",
        "shellcode"
    ],
    "description": "Elf that runs shell code. ",
    "contentFile": "elfshellcoderunner.txt",
    "customAttributes": [
        {
            "name": "compile",
            "description": "Whether or not to compile. Y/N",
            "defaultValue": "Y"
        }
    ],
    "shellCode": {
        "hasShellCode": true,
        "format": "C"
    },
    "compilation": {
        "compilationType": "C",
        "target": "elf",
        "compilationAttributes": [
             {
                "name": "saveAs",
                "description": "Name of file to be created, can be full path or just a name",
                "defaultValue": "shell.elf"
            }
        ]
    }
}