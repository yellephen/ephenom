{
    "categories": [
        "dll",
        "shell code"
    ],
    "description": "ASsembly that runs shell code.",
    "contentFile": "dllshellcoderunner.txt",
    "customAttributes" : [
        {
            "name":"assemblyName",
            "description" : "Name of the assembly.",
            "defaultValue" : "ClassLibrary1"   
        },
        {
            "name":"className",
            "description" : "Name of the class.",
            "defaultValue" : "Class1"   
        },
        {
            "name":"methodName",
            "description" : "Name of the method.",
            "defaultValue" : "runner"   
        }
    ],
    "shellCode": {
        "hasShellCode": true,
        "format": "csharp"
    },
    "compilation": {
        "compilationType": "csharp",
        "target": "library",
        "compilationAttributes": [
            {
                "name": "architecture",
                "description": "The architecture, x64 or x86.",
                "defaultValue": "x64"
            },
            {
                "name": "saveAs",
                "description": "Name of file to be created, can be full path or just a name",
                "defaultValue": "shell.dll"
            }
        ]
    }
}